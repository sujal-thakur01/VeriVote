    "contract": {
        "name": "VotingContract",
        "desc": "\n    VeriVote Smart Contract - Secure Campus Election System\n\n    Global State:\n    - candidate_a_votes: Vote count for Candidate A\n    - candidate_b_votes: Vote count for Candidate B\n    - election_start: UNIX timestamp when voting begins\n    - election_end: UNIX timestamp when voting ends\n    - total_voters: Total number of voters who have cast votes\n    - ai_report_hash: SHA256 hash of AI transparency report (32 bytes)\n    - election_closed: Boolean flag (0 = open, 1 = closed)\n\n    Local State (per voter):\n    - has_voted: Boolean flag to prevent double voting\n    - vote_timestamp: UNIX timestamp when vote was cast\n    ",
        "methods": [
            {
                "name": "create_election",
                "args": [
                    {
                        "type": "uint64",
                        "name": "start_time",
                        "desc": "UNIX timestamp for election start"
                    },
                    {
                        "type": "uint64",
                        "name": "end_time",
                        "desc": "UNIX timestamp for election end"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "string",
                    "desc": "Success message with election details"
                },
                "desc": "Create a new election with specified time window.\nOnly the contract creator can call this method."
            },
            {
                "name": "cast_vote",
                "args": [
                    {
                        "type": "uint64",
                        "name": "candidate_id",
                        "desc": "1 for Candidate A, 2 for Candidate B"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "string",
                    "desc": "Success message"
                },
                "desc": "Cast a vote for a candidate."
            },
            {
                "name": "close_election",
                "args": [
                    {
                        "type": "byte[]",
                        "name": "ai_hash",
                        "desc": "SHA256 hash of AI transparency report (must be 32 bytes)"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "string",
                    "desc": "Success message"
                },
                "desc": "Close the election and store AI transparency report hash.\nOnly the contract creator can call this method after election ends."
            },
            {
                "name": "get_results",
                "args": [],
                "readonly": true,
                "returns": {
                    "type": "(uint64,uint64,uint64,uint64,uint64,uint64,byte[])",
                    "desc": "Tuple containing all election data: - candidate_a_votes - candidate_b_votes - total_voters - election_start - election_end - election_closed (0 or 1) - ai_report_hash (empty bytes if not closed)"
                },
                "desc": "Get current election results and status.\nThis is a read-only method that can be called anytime."
            },
            {
                "name": "get_voter_status",
                "args": [],
                "readonly": true,
                "returns": {
                    "type": "(uint64,uint64)",
                    "desc": "Tuple containing: - has_voted (0 or 1) - vote_timestamp (UNIX timestamp, 0 if not voted)"
                },
                "desc": "Get the voting status of the current sender.\nThis is a read-only method for checking local state."
            },
            {
                "name": "opt_in_voter",
                "args": [],
                "readonly": false,
                "returns": {
                    "type": "string",
                    "desc": "Success message"
                },
                "desc": "Opt-in a voter to the contract (initializes local state).\nMust be called before a voter can cast their vote."
            }
        ],
        "networks": {}
    },
    "bare_call_config": {
        "no_op": "CREATE"
    }
}
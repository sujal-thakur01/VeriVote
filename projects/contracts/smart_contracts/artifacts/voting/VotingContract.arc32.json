        "global": {
            "declared": {
                "ai_report_hash": {
                    "type": "bytes",
                    "key": "ai_report_hash"
                },
                "candidate_a_votes": {
                    "type": "uint64",
                    "key": "candidate_a_votes"
                },
                "candidate_b_votes": {
